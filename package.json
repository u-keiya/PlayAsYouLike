{
  "name": "play-as-you-like",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "run-p --continue-on-error --print-label lint:*",
    "lint:eslint": "eslint .",
    "lint:secretlint": "secretlint --maskSecrets --secretlintignore .gitignore '**/*'",
    "lint:prettier-check": "prettier --cache --check .",
    "fix": "run-s --continue-on-error fix:*",
    "fix:prettier-fix": "prettier --cache --write .",
    "fix:lint-fix": "eslint --fix .",
    "start": "next start"
  },
  "dependencies": {
    "next": "^15.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@next/eslint-plugin-next": "^15.5.4",
    "@rushstack/eslint-patch": "^1.13.0",
    "@secretlint/secretlint-rule-preset-recommend": "^11.2.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-config-next": "^15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-redos": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.4.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "secretlint": "^11.2.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0"
  }
}
